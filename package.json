{
	"name": "reacthotloader",
	"version": "1.0.0",
	"main": "index.js",
	"scripts": {
		"dev": "webpack-dev-server --hot --history-api-fallback --open"
	},
	"license": "MIT",
	"dependencies": {
		"babel-core": "^6.25.0",
		"babel-loader": "^7.1.1",
		"babel-preset-es2015": "^6.24.1",
		"babel-preset-latest": "^6.24.1",
		"babel-preset-react": "^6.24.1",
		"babel-preset-stage-3": "^6.24.1",
		"bootstrap": "^3.3.7",
		"css-loader": "^0.28.4",
		"file-loader": "^0.11.2",
		"jquery": "^3.2.1",
		"react": "^15.6.1",
		"react-bootstrap": "^0.31.2",
		"react-dom": "^15.6.1",
		"react-router-dom": "^4.1.2",
		"style-loader": "^0.18.2",
		"url-loader": "^0.5.9",
		"webpack": "^3.5.1"
	},
	"devDependencies": {
		"react-hot-loader": "3.0.0-beta.6",
		"webpack-dev-server": "^2.7.0"
	},
	"babel": {
		"presets": [
			[
				"latest",
				{
					"es2015": {
						"modules": false
					}
				}
			],
			"react",
			"stage-3"
		],
		"plugins": [
			"react-hot-loader/babel"
		]
	},
	"proxy": "http://localhost:8000"
